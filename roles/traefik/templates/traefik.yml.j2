entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"

providers:
  file:
    directory: /etc/traefik/dynamic
    watch: true

certificatesResolvers:
  letsencrypt:
    acme:
      email: "{{ traefik_acme_email }}"
      storage: /var/lib/traefik/acme.json
      httpChallenge:
        entryPoint: web

log:
  level: "{{ traefik_log_level }}"
